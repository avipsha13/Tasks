#include<stdio.h>
int main()
{
	char name[100][20];
	int age[20];
	int number[20];
	int salary[20];
	
	int i=0;
	for(i=0;i<20;i++)
	{
		printf("%d",i);
		printf("Enter Name: ");
		scanf("%s",name[i]);
		printf("Enter Age: ");
		scanf("%d",&age[i]);
		printf("Enter Number: ");
		scanf("%D",&number[i]);
		printf("Enter Salary: ");
		scanf("%d",&salary[i]);
	}
	return 0;
	
}
